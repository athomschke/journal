{
  "name": "Journal",
  "version": "0.0.1",
  "private": true,
  "jest": {
    "setupEnvScriptFile": "./react-native.jestSupport.env.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils",
      "react-native-router-flux",
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "redux",
      "redux-thunk",
      "fbjs",
      "app"
    ],
    "collectCoverage": false,
    "verbose": true
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest  --no-cache",
    "test:watch": "npm run test -- --watch",
    "coverage": "npm run test -- --coverage"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "react": "^15.1.0",
    "react-native": "0.27.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-jest": "^12.1.0",
    "babel-preset-react-native": "^1.9.0",
    "jest-cli": "^12.1.1"
  }
}
